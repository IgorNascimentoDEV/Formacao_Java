package heranca_java;

public class TesteSistema {

	public static void main(String[] args) {
		
		Gerente g = new Gerente();
		g.setSenha(2222);
		
		Administrador adm = new Administrador();
		adm.setSalario(2222);
		
		SistemaInterno si = new SistemaInterno();
		
		si.autentica(g);
		adm.autentica(adm);
	}

}
