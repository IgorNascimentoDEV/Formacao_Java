package heranca_java;

public class Gerente extends Funcionario{

	private int senha;
	
	public Gerente() {
		
	}
	
	//metodos
	
	public double getBonificacao() {
		return this.salario * 0.1;
	}
	
	
	public boolean autentica(int senha) {
		if(this.senha == senha) {
			return true;
		}else {
			return false;
		}
	}
	
	public void setSenha(int senha) {
		this.senha = senha;
	}
}
